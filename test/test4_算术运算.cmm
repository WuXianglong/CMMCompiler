int a ; 
a = 2 * 4;	//a = 8
if( a<> 8)
	write(0);
else
	write(1);

real r ;
r = 2 * (3.0 - 2.10) - 0.9 * (2.50 / 1.25 );  //r = 0.0

if( r <> 0)
	write(0);
else
	write(1);

real b = 4.000001;
write(b);

int x = 60 * 60 * 24 ;
int y = 60 * 60 ;
write(x / y); 

/*output:
 *1
 *0
 *4.000001
 *24
 *///:~