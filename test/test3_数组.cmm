real realArray[6]; //声明实数数组
int  intArray [2]; //声明整形数组
real a = 2.0;
int b = 0;

intArray[0] = 0;
intArray[1] = 1;

//给数组赋值
realArray[b] = 2;
realArray[1] = a;
realArray[2] = intArray[0];
a = 0.9;
realArray[3] = a;
realArray[4] = 5 - 2 / (4.0 - 3) * 2.5 + 0.01; 

read(realArray[5]);

//打印数组
int x = 0;
while(x < 6){
	write(realArray[x]);
	x = x + 1;
}

